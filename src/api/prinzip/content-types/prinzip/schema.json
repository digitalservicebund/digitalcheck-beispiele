{
  "kind": "collectionType",
  "collectionName": "prinzips",
  "info": {
    "singularName": "prinzip",
    "pluralName": "prinzips",
    "displayName": "Prinzip",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Beschreibung": {
      "type": "blocks",
      "required": true
    },
    "Nummer": {
      "type": "integer",
      "required": true
    },
    "URLBezeichnung": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "GuteUmsetzungen": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::digitalcheck.digitalcheck"
    },
    "Kurzbezeichnung": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "order": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "PrinzipienAnwendung": {
      "type": "component",
      "component": "shared.prinzipien-anwendung",
      "repeatable": true
    },
    "Example": {
      "type": "component",
      "component": "shared.example-absatz",
      "repeatable": false
    },
    "Beispielvorhaben": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::regelungsvorhaben.regelungsvorhaben"
    },
    "Beispiel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::absatz.absatz"
    }
  }
}
