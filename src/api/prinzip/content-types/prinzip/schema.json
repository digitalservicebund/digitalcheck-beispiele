{
  "kind": "collectionType",
  "collectionName": "prinzips",
  "info": {
    "singularName": "prinzip",
    "pluralName": "prinzips",
    "displayName": "Prinzip",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Beschreibung": {
      "type": "blocks",
      "required": true
    },
    "Nummer": {
      "type": "integer",
      "required": true
    },
    "URLBezeichnung": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "Kurzbezeichnung": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "order": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "Beispielvorhaben": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::beispielvorhaben.beispielvorhaben"
    },
    "Beispiel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::absatz.absatz"
    },
    "Aspekte": {
      "type": "component",
      "component": "shared.prinzip-aspekt",
      "repeatable": true
    }
  }
}
